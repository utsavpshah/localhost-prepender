{
    "name": "Localhost Open Port",
    "description": "Press Ctrl+Shift+F to enter the port number.",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/16.png",
            "32": "images/32.png",
            "128": "images/128.png"
        }
    },
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "128": "images/128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            }
        }
    }
}